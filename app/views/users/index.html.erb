<h1>Your feed</h1>

<% @posts.each do |post| %>
  <% if post.main_image.attachment != nil %>
    <%= image_tag post.main_image.variant(resize_to_limit: [250, nil]) %>
  <% end %>

  <h1><%= post.caption %></h1>
  <p><%= link_to post.user.username, user_path(post.user) %></p>
  <p><%= link_to "Show more", user_post_path(post.user, post) %></p>
<% end %>
<%= link_to  "Submit Post", new_user_post_path(@user) %>
